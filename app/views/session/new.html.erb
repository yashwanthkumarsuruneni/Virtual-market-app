<h1>User Login</h1>
<!-- app/views/sessions/new.html.erb -->
<%= form_tag '/login' do %>
  <dl class="dl-horizontal" style="font-size:20px">
    <dt>Email:</dt>
    <dd><%= text_field_tag :email %></dd>
    <br/>
    <dt>Password:</dt>
    <dd><%= password_field_tag :password %></dd>
    <dt>
      <%= submit_tag "Submit" %>
    </dt>
  </dl>
<% end %>

